#include<stdio.h>
int main()
{
	int n,*p;
	printf("\ninput number of element in an array");
	scanf("%d",&n);
	int a[n];
	for(int i=0;i<n;i++){
	scanf("%d",&a[i]);
   }
	p=a;
	printf("Array after revrsal is \n");
	for(int i=n-1;i>=0;i--)
	printf("%d ",p[i]);
	return 0;
}
